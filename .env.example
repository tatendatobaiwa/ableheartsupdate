# AbleHearts Foundation - Environment Variables Template
# ðŸ“‹ Copy this file to .env and fill in your actual values

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Google Analytics
VITE_GA4_MEASUREMENT_ID=your_ga4_measurement_id

# Application Environment
NODE_ENV=development
VITE_APP_NAME=AbleHearts Foundation
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_BASE_URL=https://api.ablehearts.org
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Security Settings
VITE_ENABLE_SECURITY_HEADERS=true
VITE_ENABLE_CSRF_PROTECTION=true
VITE_ENABLE_SECURITY_LOGGING=false